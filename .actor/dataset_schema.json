{
    "actorSpecification": 1,
    "title": "Google Maps Extracted Data",
    "description": "Business data extracted from Google Maps including company names, star ratings, review counts, phone numbers, websites, physical addresses, and operating hours.",
    "fields": {
        "type": "object",
        "properties": {
            "name": {
                "type": "string",
                "description": "Business name"
            },
            "url": {
                "type": "string",
                "description": "Google Maps URL for the business"
            },
            "rating": {
                "type": ["number", "null"],
                "description": "Average rating (0-5 stars)"
            },
            "reviewCount": {
                "type": ["integer", "null"],
                "description": "Total number of reviews"
            },
            "category": {
                "type": ["string", "null"],
                "description": "Business category or type"
            },
            "address": {
                "type": ["string", "null"],
                "description": "Full street address"
            },
            "phone": {
                "type": ["string", "null"],
                "description": "Phone number"
            },
            "website": {
                "type": ["string", "null"],
                "description": "Business website URL"
            },
            "hoursStatus": {
                "type": ["string", "null"],
                "description": "Current open/closed status and hours"
            },
            "priceLevel": {
                "type": ["string", "null"],
                "description": "Price level indicator"
            },
            "plusCode": {
                "type": ["string", "null"],
                "description": "Google Plus Code for location"
            },
            "scrapedAt": {
                "type": "string",
                "description": "Timestamp when the data was scraped"
            },
            "isSponsored": {
                "type": "boolean",
                "description": "Whether this is a sponsored/ad listing"
            },
            "reviews": {
                "type": ["array", "null"],
                "description": "Array of reviews for this business (when extractReviews is enabled)",
                "items": {
                    "type": "object",
                    "properties": {
                        "reviewerName": {
                            "type": "string",
                            "description": "Name of the reviewer"
                        },
                        "reviewerSubtitle": {
                            "type": ["string", "null"],
                            "description": "Reviewer subtitle (e.g., 'Local Guide · 26 reviews · 3 photos')"
                        },
                        "rating": {
                            "type": ["integer", "null"],
                            "description": "Star rating given by the reviewer (1-5)"
                        },
                        "reviewText": {
                            "type": ["string", "null"],
                            "description": "The review text content"
                        },
                        "reviewDate": {
                            "type": ["string", "null"],
                            "description": "When the review was posted (relative date like '2 months ago')"
                        },
                        "likesCount": {
                            "type": ["integer", "null"],
                            "description": "Number of likes on the review"
                        },
                        "shareLink": {
                            "type": ["string", "null"],
                            "description": "Shareable link for this review (when extractReviewShareLink is enabled)"
                        }
                    },
                    "required": ["reviewerName"]
                }
            }
        },
        "required": ["name", "url", "scrapedAt"]
    },
    "views": {
        "overview": {
            "title": "Overview",
            "transformation": {
                "fields": [
                    "name",
                    "rating",
                    "reviewCount",
                    "category",
                    "address",
                    "phone",
                    "website",
                    "hoursStatus",
                    "isSponsored",
                    "url"
                ]
            },
            "display": {
                "component": "table",
                "properties": {
                    "name": {
                        "label": "Business Name",
                        "format": "text"
                    },
                    "rating": {
                        "label": "Rating",
                        "format": "number"
                    },
                    "reviewCount": {
                        "label": "Reviews",
                        "format": "number"
                    },
                    "category": {
                        "label": "Category",
