                    },
                    "website": {
                        "label": "Website",
                        "format": "link"
                    },
                    "hoursStatus": {
                        "label": "Hours",
                        "format": "text"
                    },
                    "isSponsored": {
                        "label": "Sponsored",
                        "format": "boolean"
                    },
                    "url": {
                        "label": "Google Maps URL",
                        "format": "link"
                    }
                }
            }
        },
        "detailed": {
            "title": "Detailed View",
            "transformation": {
                "fields": [
