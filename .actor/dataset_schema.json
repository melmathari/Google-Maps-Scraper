                    },
                    "priceLevel": {
                        "label": "Price Level",
                        "format": "text"
                    },
                    "plusCode": {
                        "label": "Plus Code",
                        "format": "text"
                    },
                    "isSponsored": {
                        "label": "Sponsored",
                        "format": "boolean"
                    },
                    "url": {
                        "label": "Google Maps URL",
                        "format": "link"
                    },
                    "scrapedAt": {
                        "label": "Scraped At",
                        "format": "date"
                    }
                }
            }
        }
    }
