{
    "actorSpecification": 1,
    "title": "Google Maps Extracted Data",
    "description": "Business data extracted from Google Maps including company names, star ratings, review counts, phone numbers, websites, physical addresses, and operating hours.",
    "fields": {
        "type": "object",
        "properties": {
            "name": {
                "type": "string",
                "description": "Business name"
            },
            "url": {
                "type": "string",
                "format": "uri",
                "description": "Google Maps URL for the business"
            },
            "rating": {
                "type": "number",
                "minimum": 0,
                "maximum": 5,
                "description": "Average rating (0-5 stars)"
            },
            "reviewCount": {
                "type": "integer",
                "minimum": 0,
                "description": "Total number of reviews"
            },
            "category": {
                "type": "string",
                "description": "Business category or type"
            },
            "address": {
                "type": "string",
                "description": "Full street address"
            },
            "phone": {
                "type": "string",
                "description": "Phone number"
            },
            "website": {
                "type": "string",
                "format": "uri",
                "description": "Business website URL"
            },
            "hoursStatus": {
                "type": "string",
                "description": "Current open/closed status and hours"
            },
            "priceLevel": {
                "type": "string",
                "description": "Price level indicator"
            },
            "plusCode": {
                "type": "string",
                "description": "Google Plus Code for location"
            },
            "scrapedAt": {
                "type": "string",
                "format": "date-time",
                "description": "Timestamp when the data was scraped"
            },
            "isSponsored": {
                "type": "boolean",
                "description": "Whether this is a sponsored/ad listing"
            }
        },
        "required": ["name", "url", "scrapedAt"]
    },
    "views": {
        "overview": {
            "title": "Overview",
            "transformation": {
                "fields": [
                    "name",
